#include "timer.h"
#include <avr/io.h>
#ifdef _SIMULATE_
#include "simAVRHeader.h"
#endif


//VideoLink: https://drive.google.com/file/d/1sZ8UitQzBRPNYYf64J8adfZ385IMaZU2/view?usp=sharing

enum BL_States { BL_Start, Led1, Led2, Led3, Led4, Stay, Restart } BL_State;

void TickFct_Blink() {

   switch ( BL_State ) { //Transitions
      case BL_Start:
         BL_State = Led1; //Initial state
         break;
      case Led1:
        if ((PINA & 0x01) == 0x00){
                BL_State = Stay;
        }
        else{  BL_State = Led2; }
         break;
      case Led2:
          if ((PINA & 0x01) == 0x00){
                BL_State = Stay;
        }
        else{ BL_State = Led3; }
         break;
      case Led3:
           if ((PINA & 0x01) == 0x00){
                 BL_State = Stay;
               }
           else{
             BL_State = Led4;
           }
           break;
      case Led4:
           if ((PINA & 0x01) == 0x00){
                 BL_State = Stay;
              }
           else{
            BL_State = Led1;
           }
           break;
      case Stay:
          if ((PINA & 0x01) == 0x00){
                 BL_State = Stay;
          }
          else{
                BL_State = Restart;
          }
        break;
"source/main.c" 120L, 2497C
